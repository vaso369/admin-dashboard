{
  "name": "constraint",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.2",
    "prop-types": "^15.7.2",
    "puppeteer": "^5.3.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-circular-progressbar": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build:css": "node-sass src/ -o src/ ",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "start:js": "react-scripts start",
    "start": "npm-run-all figma -p watch:css start:js ",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "pretty": "prettier --semi false --print-width 100 --single-quote --trailing-comma all --write \"src/**/*.js\"",
    "precommit": "lint-staged && npm run test",
    "test:unit": "jest unit.*test.js",
    "test:int": "react-scripts test .*test.js",
    "test:UI": "jest UI.*test.js",
    "test": "npm-run-all test:unit test:int",
    "commit": "git-cz",
    "new:comp": "npx generate-react-cli component",
    "new:comp:page": "npx generate-react-cli component --type=page",
    "new:comp:layout": "npx generate-react-cli component --type=layout",
    "new:redux:files": "node src/utilities/redux-generator.js",
    "release": "standard-version",
    "figma": "node figma/main.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "since 2010"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-jsx"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@iconify/icons-bi": "^1.0.6",
    "@iconify/react": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "fs": "0.0.1-security",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-redux-test-utils": "^0.2.0",
    "standard-version": "^9.0.0"
  }
}
